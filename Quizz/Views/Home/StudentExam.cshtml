@model Quizz.Models.TBL_CATEGORY
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <title>StudentExam</title>
</head>
<body>
    @if (@ViewBag.error != null)
    {
        <div class="alert alert-danger">
            <strong>Danger !</strong> @ViewBag.error
        </div>

    }


    @using (Html.BeginForm())
    {
        <div class="container">
            <br />
            <br />

            <h1 style="text-align:center"> Enter Quiz Number</h1>

            <input type="text" id="room" name="room" placeholder="Quizz number" class="form-control" />

            <input type="submit" value="SUBMIT" class="btn btn-info btn-block" />

        </div>

    }
    <div>
        @Html.ActionLink("Return back to dashboard", "StudentDashboard")
    </div>

    <table class="table table-responsive">
        <thead>
            <tr>

                <th>NAME</th>
                <th>Quizz Number</th>
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewData["list"] as List<Quizz.Models.TBL_CATEGORY>)
            {
                <tr>

                    <td>@item.CAT_NAME</td>
                    <td>@item.CAT_ENCRYPTEDSTRING</td>

                </tr>

            }

        </tbody>

    </table>
</body>
</html>
